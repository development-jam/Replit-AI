// Mathematical operations in Replit
print "=== Math Demo ===";

// Basic arithmetic
print "Addition: " + (5 + 3);
print "Subtraction: " + (10 - 4);
print "Multiplication: " + (6 * 7);
print "Division: " + (20 / 4);
print "Modulo: " + (17 % 5);

// Complex expressions
print "Complex: " + ((2 + 3) * (4 - 1));
print "Precedence: " + (2 + 3 * 4);

// Comparisons
print "5 > 3: " + (5 > 3);
print "2 < 1: " + (2 < 1);
print "4 == 4: " + (4 == 4);
print "3 != 5: " + (3 != 5);